    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "flowise chrome",
            "url": "http://localhost:4008",
            "webRoot": "${workspaceFolder}/packages/server/"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Flowise server",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/packages/server/bin/run",
            "args":[
                "start"
            ]
        }
    ]
}